{
  "kind": "collectionType",
  "collectionName": "users",
  "info": {
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "configurable": false
    },
    "email": {
      "type": "email",
      "configurable": false
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "configurable": false
    },
    "firstName": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string",
      "required": true
    },
    "dateOfBirth": {
      "type": "date",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "years_of_experience": {
      "type": "integer"
    },
    "facility": {
      "type": "string"
    },
    "specialisation": {
      "type": "string"
    },
    "languages": {
      "type": "string"
    },
    "awards": {
      "type": "json"
    },
    "cases": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::case.case",
      "mappedBy": "chew"
    },
    "doctor_appoint": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::appointment.appointment",
      "mappedBy": "doctor"
    },
    "patient_appoint": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::appointment.appointment",
      "mappedBy": "patient"
    },
    "gender": {
      "type": "string"
    },
    "home_address": {
      "type": "string"
    },
    "nearest_bus_stop": {
      "type": "string"
    },
    "picture_url": {
      "type": "string"
    },
    "doctor_availabilities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::availability.availability",
      "mappedBy": "users_permissions_user"
    },
    "about": {
      "type": "text"
    },
    "profile_picture": {
      "type": "string"
    },
    "transactionPin": {
      "type": "password"
    },
    "qualifications": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::qualification.qualification",
      "mappedBy": "user"
    },
    "payments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::payment.payment",
      "mappedBy": "user"
    },
    "otpCode": {
      "type": "string"
    },
    "consultation_fee": {
      "type": "biginteger"
    }
  }
}
